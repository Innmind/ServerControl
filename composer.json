{
    "name": "innmind/server-control",
    "type": "library",
    "description": "Allows to control the server from php",
    "homepage": "http://github.com/Innmind/ServerControl",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ServerControl/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/immutable": "~3.0",
        "symfony/process": "~4.0|~5.0",
        "innmind/stream": "~2.0",
        "innmind/url": "~3.0",
        "psr/log": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Server\\Control\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Server\\Control\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "vimeo/psalm": "~4.1",
        "innmind/black-box": "^4.12"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    }
}
