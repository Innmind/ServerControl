{
    "name": "innmind/server-control",
    "type": "library",
    "description": "Allows to control the server from php",
    "homepage": "http://github.com/Innmind/ServerControl",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ServerControl/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/immutable": "^4.1.1",
        "innmind/stream": "~3.0",
        "innmind/url": "~4.0",
        "psr/log": "~3.0",
        "innmind/time-continuum": "^3.0",
        "innmind/filesystem": "^5.0",
        "innmind/time-warp": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Server\\Control\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Server\\Control\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.13",
        "vimeo/psalm": "~4.17",
        "innmind/black-box": "^4.17",
        "innmind/coding-standard": "~2.0"
    }
}
