{
    "name": "innmind/server-control",
    "type": "library",
    "description": "Allows to control the server from php",
    "homepage": "http://github.com/Innmind/ServerControl",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/ServerControl/issues"
    },
    "require": {
        "php": "~8.0",
        "innmind/immutable": "dev-next",
        "symfony/process": "~5.0",
        "innmind/stream": "dev-next",
        "innmind/url": "dev-next",
        "psr/log": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Innmind\\Server\\Control\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Server\\Control\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.1",
        "innmind/black-box": "^4.12",
        "innmind/coding-standard": "^1.0"
    }
}
